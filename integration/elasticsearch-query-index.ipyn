   "source": [
    "from elasticsearch import Elasticsearch, helpers\n",
    "\n",
    "es = Elasticsearch([{'host': 'elasticsearch-logging.kube-system.svc.cluster.local',  'port':9200}])\n",
    "es_index = \"occupancy-wifi-aggregated\"\n",
    "documento = \"_doc\"\n",
    "\n",
    "zone = \"RJ_Office_Nord-Est\"\n",
    "\n",
    "body = {\n",
    "#         \"query\": {\n",
    "#         \"match\" : { \"ZONE\" : zone } \n",
    "#              }\n",
    "        }\n",
    "\n",
    "res = helpers.scan(\n",
    "                client = es,\n",
    "                scroll = '2m',\n",
    "                query = body, \n",
    "                index = es_index)\n",
    "    \n",
    "with open('occupany-data-dump.txt', 'w') as f:\n",
    "    for row in res:\n",
    "        f.write(\"%s\\n\" % str(row)) \n"
   ]
